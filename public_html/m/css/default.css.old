/***********************************************************************************************************************
	Reset styles
***********************************************************************************************************************/
* {
	-webkit-text-size-adjust:none;
}
html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, font, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center, fieldset, form, legend {
	margin:0;
	padding:0;
	border:0;
	outline:0;
	vertical-align:baseline;
	background:transparent;
}
:focus {
	outline:0;
}
ins {
	text-decoration:none;
}
del {
	text-decoration:line-through;
}
html {
	height:100%;
}
header, footer, article, section, hgroup, nav, figure, aside{
	display: block;
}
/***********************************************************************************************************************
	Main styles
***********************************************************************************************************************/
body {
	font:normal 15px/1.2 arial, helvetica, sans-serif;
	color:#333030;
	background:#fff;
	min-width:310px;
	max-width:800px;
	margin:0 auto;
}
input, textarea, select {
	font:normal 18px arial, helvetica, sans-serif;
}
select {
	-webkit-appearance:none;
	appearance:none;
	background:url(/images/arrow-select.gif) no-repeat 100% 50%;
}
input[type="text"],
input[type="password"],
input[type="email"],
input[type="tel"],
input[type="number"],
input[type="submit"],
input[type="button"],
input[type="search"]{
	-webkit-appearance:none;
}
::-webkit-input-placeholder{
	color:#000;
}
:-moz-placeholder {
	color:#000;
}
img {
	clear:both;
	margin:0 0 2px 0;
	border:0;
}
h1 {
	font-size:21px;
	line-height:24px;
	margin:25px 0 12px;
}
h2 {
	font-size:19px;
	line-height:22px;
	margin:17px 0 21px;
}
h3 {
	font-size:17px;
	line-height:20px;
	margin:17px 0 21px;
}
h4 {
	font-size:15px;
	margin:17px 0 18px;
}
h1, h2, h3, h4 {font-family:"HelveticaNeue", arial, sans-serif;}
a {
	color:#2A7BB6;
	text-decoration:none;
}
p {
	padding:0;
	margin:0 0 12px;
}
p a {text-decoration:underline;}
dl {margin:9px 0 18px;}
dd {margin:0 0 0 35px;}
ul {
	list-style: none;
	padding:0;
	margin:0;
}
.mt-content-block ul {
	list-style:square inside;
	margin:0 0 20px 5px;
}
ul li {
	padding:0;
}
.mt-content-block ul li {
	padding:0 0 0 14px;
}
ol {
	margin:0 0 20px 17px;
}
li {margin:8px 0 0;}
pre {
	display:block;
	border:5px solid #e8e8e1;
	padding:13px 17px;
	margin:33px 0 24px;
	line-height:21px;
	overflow: auto;
}
blockquote {
	margin:0 0 9px;
	padding:0 0 0 52px;
	/*background:url(/images/bg-quote.png) no-repeat 7px 0;*/
	min-height:31px;
}
q {
	quotes:none;
	margin:0 0 9px;
	display:block;
}
q:before {content:"";}
q:after {content:"";}
table {
	width:100%;
	color:#333030;
	font:14px/16px "HelveticaNeue", arial, sans-serif;
	background:#e3e3e3;
	border-collapse:collapse;
}
table.mt-table-clean {
	width: auto;
	color:inherit;
	font:inherit;
	background:none;
	border-collapse:separate;
}
table caption {
	background:#f3f3f3 url(/images/pxl.gif) repeat-x;
	font-size:22px;
	line-height:24px;
	font-weight:bold;
	padding:8px 10px;
	border:solid #d7d7d0;
	border-width:1px 1px 0;
	text-align:left;
	text-shadow:1px 1px 1px #fff;
}
table.mt-table-clean caption {
	background:none;
	font:inherit;
	font-weight:inherit;
	padding:auto;
	border:none;
	text-shadow:none;
}
table th {
	border:solid #bcbcbc;
	border-width:1px 1px 0 0;
	font-size:15px;
	line-height:18px;
	background:#cbcbcb url(/images/bg-table-border.png) repeat-y;
	padding:3px 10px;
	font-weight: bold;
}
table.mt-table-clean th {
	border: none;
	font: inherit;
	background: none;
	padding: auto;
	font-weight: bold;
}
table th:first-child, table td:first-child {background-image:none;}
table th, table td {
	text-align:left;
	vertical-align:top;
}
table td {
	border-right:1px solid #bcbcbc;
	border-top:1px solid #fff;
	background:url(/images/bg-table-border.png) repeat-y;
	padding:3px 10px;
}
table tr:nth-of-type(odd) td {
	border-color:#b6b6b6;
	background-color:#cbcbcb;
}
table.mt-table-clean td,
table.mt-table-clean tr:nth-of-type(odd) td{
	border: none;
	background: none;
	padding: auto;
}
/***********************************************************************************************************************
	Predefined Template styles
***********************************************************************************************************************/
.mt-tab-nav,
.mt-breadcrumbs,
.mt-paging,
.mt-single-link,
.mt-links a,
.mt-menu li a,
.mt-custom-form .mt-btn,
.mt-search,
.mt-form .mt-btn,
.mt-form-field,
.mt-accordion header{
	background-image: -webkit-gradient(
		linear,
		left top,
		left bottom,
		color-stop(0%,rgba(255,255,255,.2)),
		color-stop(100%,rgba(0,0,0,.2))
	);
	background-image: -webkit-linear-gradient(
		top,
		rgba(255,255,255,.2) 0%,
		rgba(0,0,0,.2) 100%
	);
	background-image: -moz-linear-gradient(
		top,
		rgba(255,255,255,.2) 0%,
		rgba(0,0,0,.2) 100%
	);
	background-image: linear-gradient(
		top,
		rgba(255,255,255,.2) 0%,
		rgba(0,0,0,.2) 100%
	);
}
.mt-indent {
	margin-left:5px !important;
	margin-right:5px !important;
}
/* expandable content */
.mt-expandable-content {
	margin:0;
	border:1px solid #e4e4e4;
	border-top-color:#c4c4c4;
}
.mt-expandable-content .mt-opener {
	background:#c3c3c3 url(/images/bg-expandable-content.png) no-repeat 100% 0;
	border:solid #f2f2f2;
	border-width:1px 0 0;
	display:block;
	height:36px;
	padding:13px 53px 0 0;
	font-size:17px;
	line-height:22px;
	font-weight:bold;
	text-shadow:1px 1px 1px #fff;
	overflow:hidden;
	white-space:nowrap;
	color:#333;
}
.mt-expandable-content.mt-open .mt-opener {background-position:100% -50px;}
.mt-expandable-content .mt-opener span {
	background:#c3c3c3 url(/images/bg-expandable-content.png) no-repeat 100% -99px;
	padding:0 16px 0 13px;
	display:inline-block;
}
.mt-content {padding:0 0 1px;}
.mt-expandable-content .mt-gallery {margin:0 !important;}
/* gallery */
.mt-gallery {
	margin:2px;
	-webkit-border-radius:5px;
	-moz-border-radius:5px;
	border-radius:5px;
	border:1px solid #ccc;
	position:relative;
	padding:5px 5px 31px;
	background:#fff url(/images/bg-galley-control-panel.gif) repeat-x 0 100%;
}
.mt-gallery div.holder {overflow:hidden;}
.mt-gallery div.holder:after {
	clear:both;
	display:block;
	content:'';
}
.mt-gallery .mt-list {
	margin:0;
	width:9999px;
	overflow:hidden;
	background:#fff;
	list-style: none;
}
.mt-gallery .mt-list li.mt-list-item {
	padding:0 0 5px;
	background:none;
	float:left;
	text-align:center;
	color:#686868;
	margin:0;
	/*width:150px; */
	text-align: center;
}
.mt-gallery .mt-list .mt-item {margin: 0 auto;}
.mt-gallery li.mt-list-item a {color:#686868;}
.mt-gallery li.mt-list-item img {
	display:block;
	clear:none;
	margin: 0 auto;
}
.mt-gallery li.mt-list-item div.item {margin: 0 auto;}
.mt-gallery .mt-prev, .mt-gallery .mt-next {
	width:89px;
	height:29px;
	text-indent:0;
	overflow:hidden;
	background:#e2e2e2 url(/images/btn-gallery.png) no-repeat;
	position:absolute;
	z-index:2;
	bottom:0;
	padding:0 0 0 26px;
	font:bold 15px/31px "HelveticaNeue", arial, sans-serif;
	color:#333;
	margin:0;
	border:0;
}
.mt-gallery .mt-prev:after,
.mt-gallery .mt-next:after,
.mt-gallery .mt-prev:before,
.mt-gallery .mt-next:before{
	display:none;
}
.mt-gallery .mt-prev {left:0;text-align: left !important;}
.mt-gallery .mt-next {
	right:0;
	background-position:100% 0;
	padding:0 26px 0 0;
	text-align:right;
}
.mt-gallery .mt-counter {
	font:bold 15px/31px "HelveticaNeue", arial, sans-serif;
	position: absolute;
	bottom: 0;
	left: 50%;
	width: 40px;
	margin-left: -20px;
	text-align: center;
}
/*template:tabs*/
.mt-tab-nav {
	font:15px Arial, Helvetica, sans-serif;
	border:solid #d7d7d0;
	border-width:0 1px;
	font-weight:bold;
	height:33px;
	line-height:31px;
	margin:0 0 3px;
	background-color:#e2e2e2;
}
.mt-tab-nav nav {
	overflow:hidden;
	height:37px;
}
.mt-tab-nav nav:after {
	content:"";
	clear:both;
	display:block;
}
.mt-tab-nav ul {
	padding:0 0 0 3px;
	margin:0;
	width:9999px;
	height:33px;
	list-style: none;
}
.mt-tab-nav li {
	float:left;
	padding:1px 3px 0 3px !important;
	margin:1px -3px 0 0;
	background:none;
	position:relative;
}
.mt-tab-nav li:before {
	content:'';
	width:1px;
	height:100%;
	position: absolute;
	left:0;
	top:0;
	background:border:solid rgba(255,255,255,.9);;
	border:solid rgba(255,255,255,.6);
	border-width:0 1px;
}
.mt-tab-nav li:first-child:before {display:none;}
.mt-tab-nav li a {
	color:#333030;
	display:block;
	padding:0 8px;
	margin:0 3px 0 2px;
	position:relative;
	z-index:2;
}
.mt-tab-nav li.selected:before,
.mt-tab-nav li.selected + li:before{display:none;}
.mt-tab-nav li.selected a{
	border-left:1px solid #c6c6c6;
	border-right:1px solid #c6c6c6;
	border-top:1px solid #8e8e8e;
	-webkit-border-top-left-radius:4px;
	-webkit-border-top-right-radius:4px;
	-moz-border-radius-topleft:4px;
	-moz-border-radius-topright:4px;
	border-top-left-radius:4px;
	border-top-right-radius:4px;
	background:#fff;
	margin:-1px 3px -4px 2px;
	padding:0 7px 4px;
}
.mt-tab-content {
	-webkit-border-radius:4px;
	-moz-border-radius:4px;
	border-radius:4px;
	border:1px solid #bfbfbf;
	padding:10px;
	text-align:center;
	margin:0 3px 5px;
}
/* buttons */
.mt-prev,
.mt-next,
.mt-prev-page,
.mt-next-page {
	width:29px;
	height:32px;
	text-indent:-9999px;
	background-image: -webkit-gradient(
		linear,
		left top,
		left bottom,
		color-stop(0%,rgba(0,0,0,.1)),
		color-stop(100%,rgba(0,0,0,.15))
	);
	background-image: -webkit-linear-gradient(
		top,
		rgba(0,0,0,.1) 0%,
		rgba(0,0,0,.15) 100%
	);
	background-image: -moz-linear-gradient(
		top,
		rgba(0,0,0,.1) 0%,
		rgba(0,0,0,.15) 100%
	);
	background-image: linear-gradient(
		top,
		rgba(0,0,0,.1) 0%,
		rgba(0,0,0,.15) 100%
	);
	position:relative;
	z-index:2;
	border-top:1px solid rgba(255,255,255,.6);
}
.mt-prev,
.mt-prev-page {
	float:left;
	border-right:1px solid rgba(255,255,255,.6);
	margin-left:-1px;
}
.mt-prev:after,
.mt-prev-page:after{
	content:'';
	width:0;
	height:100%;
	position:absolute;
	right:1px;
	top:0;
	overflow:hidden;
	border-left:1px solid rgba(255,255,255,.6);
}
.mt-prev:before,
.mt-prev-page:before{
	content:'';
	width:100%;
	height:100%;
	position:absolute;
	left:0;
	top:0;
	background:url(/images/btn-prev-next.png) no-repeat;
}
.mt-next,
.mt-next-page {
	float:right;
	margin-right:-1px;
	border-left:1px solid rgba(255,255,255,.6);
}
.mt-next:after,
.mt-next-page:after{
	content:'';
	width:0;
	height:100%;
	position:absolute;
	left:1px;
	top:0;
	overflow:hidden;
	border-right:1px solid rgba(255,255,255,.6);
}
.mt-next:before,
.mt-next-page:before{
	content:'';
	width:100%;
	height:100%;
	position:absolute;
	left:0;
	top:0;
	background:url(/images/btn-prev-next.png) no-repeat 0 -32px;
}
/* breadcrumbs */
.mt-breadcrumbs {
	overflow:hidden;
	position:relative;
	border-top:1px solid #e3e3e3;
	font:15px/32px "HelveticaNeue", arial, sans-serif;
	border:solid #bfbfbf;
	border-width:0 1px;
	background-color:#e2e2e2;
}
.mt-breadcrumbs nav {overflow:hidden;}
.mt-breadcrumbs ol {
	border-top:1px solid rgba(255, 255, 255, 0.6);
	margin:0;
	list-style:none;
	padding:0;
	height:32px;
	overflow:hidden;
	width:9999px;
	position:relative;
	z-index:1;
}
.mt-breadcrumbs li {
	display:inline-block;
	padding:0 0 0 11px;
	margin:0 -13px 0 0;
	background:none;
	background:url(/images/bg-breadcrumbs-item.png) no-repeat 100% 0;
}
.mt-breadcrumbs li:first-child {
	padding-left:0;
}
.mt-breadcrumbs li.mt-active:first-child {
	margin-left:0;
}
.mt-breadcrumbs li.mt-active:first-child:before {
	display:none;
}
.mt-breadcrumbs li.mt-active {
	padding:0 21px 0 10px;
	margin-left:11px;
	background:url(/images/bg-breadcrumbs-item.png) no-repeat 100% -32px;
	position:relative;
}
.mt-breadcrumbs li.mt-active:before {
	content:'';
	display:block;
	position:absolute;
	top:0;
	left:-11px;
	width:11px;
	height:100%;
	background:url(/images/bg-breadcrumbs-item.png) no-repeat 0 -64px;
}
.mt-breadcrumbs li a {
	display:inline-block;
	padding:0 21px 0 10px;
	color:#333;
}

.mt-breadcrumbs .mt-prev:after{
	width:13px;
	right:-13px;
	background-image: -webkit-gradient(
		linear,
		left top,
		right top,
		color-stop(0%,rgba(255,255,255,.8)),
		color-stop(90%,rgba(255,255,255,0))
	);
	background-image: -webkit-linear-gradient(
		left,
		rgba(255,255,255, .8) 0%,
		rgba(255,255,255, 0) 90%
	);
	background-image: -moz-linear-gradient(
		left,
		rgba(255,255,255, .8) 0%,
		rgba(255,255,255, 0) 90%
	);
	background-image: linear-gradient(
		left,
		rgba(255,255,255, .8) 0%,
		rgba(255,255,255, 0) 90%
	);
	padding: 0 0 0 1px;
	-moz-background-origin: content-box;
	-webkit-background-origin: content-box;
	background-origin: content-box;
}
.mt-breadcrumbs .mt-next:after{
	width:13px;
	left:-13px;
	background-image: -webkit-gradient(
		linear,
		right top,
		left top,
		color-stop(0%,rgba(255,255,255,.8)),
		color-stop(90%,rgba(255,255,255,0))
	);
	background-image: -webkit-linear-gradient(
		right,
		rgba(255,255,255, .8) 0%,
		rgba(255,255,255, 0) 90%
	);
	background-image: -moz-linear-gradient(
		right,
		rgba(255,255,255, .8) 0%,
		rgba(255,255,255, 0) 90%
	);
	background-image: linear-gradient(
		right,
		rgba(255,255,255, .8) 0%,
		rgba(255,255,255, 0) 90%
	);
	padding: 0 1px 0 0;
	-moz-background-origin: content-box;
	-webkit-background-origin: content-box;
	background-origin: content-box;
}
/* paging */
.mt-paging {
	height:33px;
	overflow:hidden;
	position:relative;
	font:20px/24px "HelveticaNeue", arial, sans-serif;
	border:1px solid #bfbfbf;
	border-top-color:#e3e3e3;
	background-color:#e2e2e2;
}
.mt-paging nav {overflow:hidden;}
.mt-paging ul {
	margin:0;
	list-style:none;
	padding:4px 0 0;
	overflow:hidden;
	height:29px;
	width: 9999px;
}
.mt-paging li {
	float:left;
	margin:1px 0 0;
	padding:1px 0 0 0;
	background:none;
	line-height:22px;
	background-size:auto;
}

.mt-paging li a,
.mt-paging li span {
	display:inline-block;
	padding:0 10px;
	color:#333;
}
.mt-paging li span {
	color:#fff;
}
.mt-paging li.mt-active  {
	border:1px solid #909090;
	padding:1px;
	margin:0 ;
	background-image: -webkit-gradient(
		linear,
		left top,
		left bottom,
		color-stop(0%,rgba(255,255,255,.3)),
		color-stop(100%,rgba(0,0,0,.2))
	);
	background-image: -webkit-linear-gradient(
		top,
		rgba(255,255,255,.3) 0%,
		rgba(0,0,0,.2) 100%
	);
	background-image: -moz-linear-gradient(
		top,
		rgba(255,255,255,.2) 0%,
		rgba(0,0,0,.2) 100%
	);
	background-image: linear-gradient(
		top,
		rgba(255,255,255,.3) 0%,
		rgba(0,0,0,.2) 100%
	);
}
.mt-paging li.mt-active span {
	background-image: -webkit-gradient(
		linear,
		left top,
		left bottom,
		color-stop(0%,rgba(0,0,0,.2)),
		color-stop(100%,rgba(0,0,0,.2))
	);
	background-image: -webkit-linear-gradient(
		top,
		rgba(0,0,0,.2) 0%,
		rgba(0,0,0,.2) 100%
	);
	background-image: -moz-linear-gradient(
		top,
		rgba(0,0,0,.2) 0%,
		rgba(0,0,0,.2) 100%
	);
	background-image: linear-gradient(
		top,
		rgba(0,0,0,.2) 0%,
		rgba(0,0,0,.2) 100%
	);
}
.mt-paging .mt-prev-page, .mt-paging .mt-next-page {
	width:60px;
	background-image: -webkit-gradient(
		linear,
		left top,
		left bottom,
		color-stop(0%,rgba(0,0,0,.2)),
		color-stop(100%,rgba(0,0,0,.3))
	);
	background-image: -webkit-linear-gradient(
		top,
		rgba(0,0,0,.2) 0%,
		rgba(0,0,0,.3) 100%
	);
	background-image: -moz-linear-gradient(
		top,
		rgba(0,0,0,.2) 0%,
		rgba(0,0,0,.3) 100%
	);
	background-image: linear-gradient(
		top,
		rgba(0,0,0,.2) 0%,
		rgba(0,0,0,.3) 100%
	);
	margin:0;
}
.mt-prev-page:before{
	background-position:0 -128px;
}
.mt-next-page:before{
	background-position:0 -160px;
}
/* single link */
.mt-single-link {
	display:block;
	text-align:center;
	text-decoration:none;
	border:1px solid #d7d7d0;
	border-left-color:#bfbfbf;
	border-right-color:#bfbfbf;
	background-color:#f4f4f4;
	padding:10px 0;
	font-weight:bold;
	text-shadow:1px 1px 1px #fff;
	color:#333;
	font-size: 15px;
}
/* links */
.mt-links ul {
	margin:5px 0 0;
	text-align:center;
	list-style:none;
	font-size: 15px;
}
.mt-links li,
.mt-content-block .mt-links li{
	padding:0;
	background:none;
	border:solid #d7d7d0;
	border-width:1px 0;
	display:inline-block;
	margin:0 1px 5px;
}
.mt-links a {
	border:solid #d7d7d0;
	border-width:0 1px;
	padding:0 10px;
	line-height:33px;
	margin:0 -1px;
	background-color:#f4f4f4;
	display:inline-block;
	text-shadow:1px 1px 1px #fff;
	font-weight:bold;
	white-space:nowrap;
	color:#333;
}
/* menu button*/
.mt-menu-btn {
	display:block;
	text-align:center;
	color:#333030;
	text-shadow:1px 1px 1px #fff;
	text-transform:uppercase;
	background:#e6e6e6 url(/images/bg-menu-btn.png) no-repeat 50% 0;
	overflow:hidden;
	font:bold 15px/39px "HelveticaNeue", arial, sans-serif;
