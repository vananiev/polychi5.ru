	-webkit-border-radius:10px;
	-moz-border-radius:10px;
	border-radius:10px;
	border:1px solid #b3b3b3;
	background:#fff;
	padding:4px 5px;
	font-size:14px;
	line-height:16px;
	color:#333030;
	margin:0 0 15px;
	overflow:hidden;
	display:block;
}
.mt-custom-form .mt-control input {
	height:30px;
	width:30px;
	background:#fff;
	border:1px solid #999;
	vertical-align:middle;
	float:left;
	margin:1px 5px 1px 0;
	padding:0;
}
.mt-custom-form .mt-control em {
	display:table;
	height:100%;
	font-style:normal;
}
.mt-custom-form .mt-control span {
	display:table-cell;
	vertical-align:middle;
}
/* template:ajax */
.mt-ajax-link {
	text-decoration:underline;
	font-size:18px;
}
/* template:google-map */
.mt-google-map-link {
	text-decoration:underline;
	font-size:18px;
}
.mt-google-map-show {
	width:99%;
	height:340px;
	border:1px solid #999;
}
/* template:error */
.mt-error h1 {margin-top: 12px;}
.mt-error-text {margin: 0 0 12px 0;}
/* accordion */
.mt-accordion {
	background:#fff;
	padding:0 0 1px;
	margin:0;
	text-align:center;
}
.mt-accordion section {
	padding:1px 0 0;
	margin:0;
	position:relative;
}
.mt-accordion section:first-child {
	background-image:none;
	padding-top:0;
}
.mt-accordion header {
	color:#333;
	text-shadow:1px 1px 1px #fff;
	background-color:#e2e2e2;
	font:bold 15px/18px "HelveticaNeue", arial, sans-serif;
}
.mt-accordion header a {
	display:block;
	color:#333;
	padding:10px;
}
.mt-accordion .mt-content {
	padding:5px;
	border:1px solid #d7d7d0;
	background:transparent;
}
/* main header */
.mt-main-header{
	border:1px solid #d7d7d0;
	border-left-color:#bfbfbf;
	border-right-color:#bfbfbf;
	background-color:#f4f4f4;
	padding:10px 11px;
	line-height:25px;
	background-image: -webkit-gradient(
		linear,
		left top,
		left bottom,
		color-stop(0%,rgba(160,160,160,.4)),
		color-stop(100%,rgba(0,0,0,.4))
	);
	background-image: -webkit-linear-gradient(
		top,
		rgba(160,160,160,.4) 0%,
		rgba(0,0,0,.4) 100%
	);
	background-image: -moz-linear-gradient(
		top,
		rgba(160,160,160,.4) 0%,
		rgba(0,0,0,.4) 100%
	);
	background-image: linear-gradient(
		top,
		rgba(160,160,160,.4) 0%,
		rgba(0,0,0,.4) 100%
	);
}
.mt-main-header h1{
	font-size:20px;
	line-height:24px;
	margin:0;
	color: #333030;
}
.mt-link-back,
.mt-link-home{
	float:left;
	margin:-1px 10px 0 0;
	padding:0 0 0 24px;
	height:24px;
	line-height:24px;
	font-weight:bold;
	text-shadow:1px 1px 1px #fff;
	color:#333;
	border:1px solid rgba(0,0,0,.25);
	border-top-color:rgba(255,255,255,.1);
	border-bottom-color:rgba(0,0,0,.45);
	-moz-box-shadow:0 1px 2px rgba(255,255,255,.25);
	-webkit-box-shadow:0 1px 2px rgba(255,255,255,.25);
	box-shadow:0 1px 2px rgba(255,255,255,.25);
	background-image: -webkit-gradient(
		linear,
		left top,
		left bottom,
		color-stop(0%,rgba(255,255,255,.1)),
		color-stop(100%,rgba(20,20,20,.3))
	);
	background-image: -webkit-linear-gradient(
		top,
		rgba(255,255,255,.1) 0%,
		rgba(20,20,20,.3) 100%
	);
	background-image: -moz-linear-gradient(
		top,
		rgba(255,255,255,.1) 0%,
		rgba(20,20,20,.3) 100%
	);
	background-image: linear-gradient(
		top,
		rgba(255,255,255,.1) 0%,
		rgba(20,20,20,.3) 100%
	);
	-moz-border-radius: 2px;
	-webkit-border-radius: 2px;
	border-radius: 2px;
	position: relative;
}
.mt-link-back:after,
.mt-link-home:after{
	content:'';
	width:26px;
	height:26px;
	position:absolute;
	left:-1px;
	top:50%;
	margin:-13px 0 0 0;
	background:url(/images/links.png) no-repeat;
}
.mt-link-back:after{
	background-position: 0 0;
}
.mt-link-back span,
.mt-link-home span{
	margin: 0 10px 0 0;
	font-size: 15px;
}
.mt-hidden{
	display:none;
}
.set-fontresize .mt-content-block,
.set-fontresize .mt-content,
.set-fontresize .mt-tab-content,
.set-fontresize .mt-content-block table,
.set-fontresize .mt-content table,
.set-fontresize .mt-tab-content table,
.set-fontresize .mt-content-block table th,
.set-fontresize .mt-content table th,
.set-fontresize .mt-tab-content table th{
	font-size: 1em;
	line-height:1.25;
}
.mt-link-back-hidden,
.mt-link-home-hidden {
	float: right;
	visibility: hidden;
}
/* mt-notice */
.mt-notice {
	padding: 0 5px;
	border: 1px solid #229930;
	margin: 5px;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	border-radius: 4px;
	background: #8af298;
	color:#333;
	text-align:center;
	font-size:15px !important;
	line-height:1.2;
}
.mt-notice h1, .mt-notice p {margin: 5px 0;}
.mt-notice span {white-space: nowrap;}

.mt-notice ul {
	margin:0 !important;
	padding:0 0 5px !important;
	list-style:none !important;
}
.mt-notice ul li {
	display:inline-block;
	margin:1px 2px !important;
	padding:0 !important;
	width:95px;
	background:none !important;
	border:0 !important;
	height:auto !important;
}

html body .mt-notice a {
	display:block !important;
	width:auto !important;
	text-indent:0 !important;
	margin:0 !important;
	border:1px solid #87DB93 !important;
	-webkit-border-radius:3px !important;
	-moz-border-radius:3px !important;
	border-radius:3px !important;
	text-decoration:none !important;
	color:#333030 !important;
	padding:10px 6px !important;
	white-space:nowrap !important;
	text-shadow:1px 1px 1px #fff !important;
	font:bold 15px/18px "HelveticaNeue", arial, sans-serif !important;
	background-color:transparent !important;
	background-position:0 0 !important;
	background-image: -webkit-gradient(
		linear,
		left top,
		left bottom,
		color-stop(0%,rgba(255,255,255,.2)),
		color-stop(100%,rgba(0,0,0,.2))
	) !important;
	background-image: -webkit-linear-gradient(
		top,
		rgba(255,255,255,.2) 0%,
		rgba(0,0,0,.2) 100%
	) !important;
	background-image: -moz-linear-gradient(
		top,
		rgba(255,255,255,.2) 0%,
		rgba(0,0,0,.2) 100%
	) !important;
	background-image: linear-gradient(
		top,
		rgba(255,255,255,.2) 0%,
		rgba(0,0,0,.2) 100%
	) !important;
}

.mt-notice a:after {
	background:none !important;
}

p.mt-invalid{
	margin:8px 0 0;
	color:#f00;
	font-size:12px;
	line-height:14px;
}
label.mt-control p.mt-invalid{margin-top:4px;}

@media
only screen and (-webkit-min-device-pixel-ratio: 1.5),
only screen and (min-device-pixel-ratio: 1.5) {
	.mt-social-links a {
		background-image:url(/images/icons-social-links@2.png);
		background-size:320px 40px;
	}
	.mt-fontresize a {
		background-image:url(/images/icons-fontresize@2.png);
		background-size:120px 40px;
	}
	.mt-breadcrumbs li,
	.mt-breadcrumbs li.mt-active,
	.mt-breadcrumbs li.mt-active:before {
		background-image:url(/images/bg-breadcrumbs-item.png);
		background-size:600px 96px;
	}
	.mt-call a:after{
		background-image:url(/images/bg-call-btn@2.png);
		background-size:23px 23px;
	}
	blockquote {
		background-image:url(/images/bg-quote@2.png);
		background-size:35px 31px;
	}
	.mt-logo {
		background-image:url(/images/mobilizetoday@2.png);
		background-size:150px 37px;
	}
	.mt-menu-btn {
		background-image:url(/images/bg-menu-btn@2.png);
		background-size:800px 78px;
	}
	.mt-search .mt-btn input {
		background-image:url(/images/btn-search@2.png);
		background-size:35px 26px;
	}
	.mt-service .mt-btn {
		background-image:url(/images/desktop@2.png);
		background-size:22px 18px;
	}
	.mt-expandable-content .mt-opener,
	.mt-expandable-content .mt-opener span {
		background-image:url(/images/bg-expandable-content@2.png);
		background-size:1600px 116px;
	}
	.mt-logo + .mt-btn {
		background:none;
	}
	.mt-prev:before,
	.mt-prev-page:before{
		background-image:url(/images/btn-prev-next@2.png);
		background-size:60px 256px;
	}
	.mt-next:before,
	.mt-next-page:before{
		background-image:url(/images/btn-prev-next@2.png);
		background-size:60px 256px;
	}
	.mt-link-back:after,
	.mt-link-home:after{
		background-image:url(/images/links@2.png);
		background-size:25px 13px;
	}
}