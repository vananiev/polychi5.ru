	margin:0;
	height:32px;
	padding:6px;
	border:1px solid #c5c5c5;
}
.mt-search fieldset {
	margin:0 0 -2px;
	padding:2px 68px 0 0;
	height:30px;
	position:relative;
	line-height:28px;
	-moz-border-radius:16px;
	-webkit-border-radius:16px;
	border-radius:16px;
	background:#fff;
